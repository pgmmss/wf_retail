ENGINE INT SET FCA ON
SET SHORTPATH=SQL
GRAPH FILE app02/wf_retail_lite
 HEADING
  "Report &FOCFOCEXEC"
 WRITE
   SUM.COGS_US
   BY HIGHEST 5 TOTAL SUM.COGS_US NOPRINT
   BY FULLNAME
 ON TABLE SET PAGE NOPAGE
 ON TABLE SET EMPTYREPORT ON
 ON TABLE SET SHORTPATH SQL
 ON TABLE SET HTMLCSS ON
 ON TABLE PCHOLD
  FORMAT JSCHART
 ON TABLE SET LOOKGRAPH BAR
 ON TABLE SET HTMLENCODE ON
 ON TABLE SET GRAPHDEFAULT OFF
 ON TABLE SET AUTOFIT ON
 ON TABLE SET EMBEDHEADING ON
 ON TABLE SET STYLE *
  INCLUDE=_EDAHOME/etc/Warm, $
  TYPE=REPORT, CHART-ORIENTATION=HORIZONTAL, $
  TYPE=DATA, COLUMN=N3, BUCKET=Y-AXIS, /*SUM.COGS_US*/
$
  TYPE=DATA, COLUMN=N1, BUCKET=(X-AXIS tooltip), /*SUM.COGS_US*/
$
  TYPE=DATA, COLUMN=N2, BUCKET=X-AXIS, /*FULLNAME*/
$
 ENDSTYLE
END
