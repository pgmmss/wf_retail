ENGINE INT SET TABLESAMPLING_PERCENTAGE
ENGINE INT SET FCA ON
GRAPH FILE app02/wf_retail_lite
 SUM
   SUM.COGS_US
   SUM.GROSS_PROFIT_US
   BY WF_RETAIL_GEOGRAPHY_CUSTOMER.BUSINESS_REGION
   BY PRODUCT_CATEGORY
   BY WF_RETAIL_TIME_SALES.TIME_QTR
 ON TABLE SET EMPTYREPORT ON
 ON TABLE SET SHORTPATH SQL
 ON TABLE SET HTMLCSS ON
 ON TABLE PCHOLD
  FORMAT JSCHART
 ON TABLE SET LOOKGRAPH BAR
 ON TABLE SET HTMLENCODE ON
 ON TABLE SET GRAPHDEFAULT OFF
 ON TABLE SET AUTOFIT ON
 ON TABLE SET EMBEDHEADING ON
 ON TABLE SET STYLE *
  INCLUDE=_edahome/etc/ENID_Warm, $
  TYPE=REPORT, CHART-ORIENTATION=HORIZONTAL, $
  TYPE=DATA, COLUMN=N4, /*SUM.COGS_US*/
   BUCKET=Y-AXIS, $
  TYPE=DATA, COLUMN=N5, /*SUM.GROSS_PROFIT_US*/
   BUCKET=Y-AXIS, $
  TYPE=DATA, COLUMN=N1, /*WF_RETAIL_GEOGRAPHY_CUSTOMER.BUSINESS_REGION*/
   BUCKET=page, $
  TYPE=DATA, COLUMN=N2, /*PRODUCT_CATEGORY*/
   BUCKET=slider, $
  TYPE=DATA, COLUMN=N3, /*WF_RETAIL_TIME_SALES.TIME_QTR*/
   BUCKET=X-AXIS, $
 ENDSTYLE
 HEADING
  "Report &FOCFOCEXEC"
  "BUSINESS_REGION <BUSINESS_REGION"
  " <REQUEST.FILTERS"
END
